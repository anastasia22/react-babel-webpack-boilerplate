    "react": "15.x",
    "react-dom": "15.x",
    "react-router": "^2.8.1",
    "socket.io": "^1.4.8",
    "url": "^0.11.0",
    "webpack-dev-server": "^1.16.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "6.x",
